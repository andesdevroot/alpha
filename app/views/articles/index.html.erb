<h1>Todos los artículos</h1><table>
 <tr>
  <th>Título</th>
  <th>Descripción</th>
 </tr>
 <% @articles.each do |article| %>
 <>
  <td><%= article.title %></td>
  <td><%= article.description %></td>
  <td><%= link_to 'Editar', edit_article_path(article) %></td>
  <td><%= link_to 'Ver', article_path(article) %></td>
  <td><%= link_to 'Borrar', article_path(article), method: :delete, data: {confirm: "Estás seguro?"} %></td>
 </tr>
 <% end %>
</table>

